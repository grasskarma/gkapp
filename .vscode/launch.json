{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "fetch",
      "program": "${workspaceFolder}/../screensrv/functions/lib/index.js",
      "runtimeArgs": [
        "--nolazy"
      ],
      "outFiles": [
        "${workspaceFolder}/../screensrv/lib/**/*.js"
      ],
      "sourceMaps": true
    },
    {
      "name": "Flutter",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "seeder",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "seeder (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "seeder (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Flutter (lib/main.dart)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart"
    }
  ]
}